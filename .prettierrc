{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "bracketSameLine": false,
  "jsxSingleQuote": false,
  "proseWrap": "always",
  "singleAttributePerLine": true,
  "quoteProps": "as-needed",
  "htmlWhitespaceSensitivity": "strict",
  "experimentalTernaries": true,
  "embeddedLanguageFormatting": "auto"
}
